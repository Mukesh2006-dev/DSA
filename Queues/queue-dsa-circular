#include <stdio.h>
#define size 5
int queue[size];
int front=-1,rear=-1;
void enqueue(int value){
    if(front == 0 && rear==size-1||(rear+1)%size==front){
        printf("Queue is full\n");
        return;
    }
        if(front==-1 && rear==-1){
            front=rear=0;
        }else{
            rear=(rear+1)%size;
        }
        queue[rear]=value;
        printf("Enqueued:%d\n",value);
}
void dequeue(){
    if(front==-1&&rear==-1){
        printf("The queue is Empty\n");
        return;
    }
    printf("Dequeued:%d",queue[front]);
    if(front==rear){
        front=rear=-1;
    }
    else{
        front=(front+1)%size;
    }
}
void display(){
    if(front==-1&&rear==-1){
        printf("Queue is empty");
    }
    int i=front;
    while(1){
        printf(" %d",queue[i]);
        if(i==rear)
            break;
            i=(i+1)%size;
    }
}
int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);
    enqueue(40);
    enqueue(50);
    dequeue();
    display();
    return 0;
}
