#include <stdio.h>
int swap(int *x,int *y){
    int temp=*x;
    *x=*y;
    *y=temp;
}
void Selectionsort(int A[],int n){
    int i,j,k;
    for(i=0;i<n-1;i++){
        k=i;
        for(j=i;j<n;j++){
            if(A[j]<A[k]){
                k=j;
            }
        }
        swap(&A[i],&A[k]);
    }
}
int main() {
int A[]={8,6,3,10,9,4,12,5,2,7},n=10,i;
Selectionsort(A,10);
for(i=0;i<10;i++){
    printf("%d ",A[i]);
}
printf("\n");
    return 0;
}


